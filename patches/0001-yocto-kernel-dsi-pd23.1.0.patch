diff --git a/drivers/gpu/drm/bridge/imx8mp-dsi-crosslink-oled/imx8mp-i2c-oled.c b/drivers/gpu/drm/bridge/imx8mp-dsi-crosslink-oled/imx8mp-i2c-oled.c
